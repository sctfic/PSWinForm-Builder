	"TabControl": {
		"prefix": "TabControl",
		"body": [
			"@{",
			"    ControlType = 'TabControl'",
			"    # Name        = 'Tabs'",
			"    Text        = '${1:label}'",
			"    Dock        = 'Fill'",
			"    Events      = @{",
			"        Load    = [Scriptblock]{ # Event",
			"            Invoke-EventTracer \\$this 'Load'",
			"        }",
			"        SelectedIndexChanged = [Scriptblock]{ # Event",
			"            Invoke-EventTracer \\$this 'SelectedIndexChanged'",
			"            \\$this.SelectedTab.Name",
			"        }",
			"    }",
			"    Childrens   = @(",
			"        @{",
			"            ControlType = 'TabPage'",
			"            # Name        = 'TabPage'",
			"            Text        = '${2:Tab1}'",
			"            Dock        = 'Fill'",
			"            Events      = @{",
			"                Load    = [Scriptblock]{ # Event",
			"            Invoke-EventTracer \\$this 'Load'",
			"                }",
			"            }",
			"            Childrens   = @(",
			"            )",
			"        },",
			"        @{",
			"            ControlType = 'TabPage'",
			"            # Name        = 'TabPage'",
			"            Text        = '${2:Tab2}'",
			"            Dock        = 'Fill'",
			"            Events      = @{",
			"                Load    = [Scriptblock]{ # Event",
			"            Invoke-EventTracer \\$this 'Load'",
			"                }",
			"            }",
			"            Childrens   = @(",
			"            )",
			"        }",
			"    )",
			"}"
		],
